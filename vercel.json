{
  "version": 2,
  "outputDirectory": "Frontend/dist",
  "routes": [
    {
      "src": "/api/city",
      "dest": "/api/city.js"
    },
    {
      "src": "/api/heatmap",
      "dest": "/api/heatmap"
    },
    {
      "src": "/api/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/gemini/chat",
      "dest": "/api/gemini/chat"
    },
    {
      "src": "/api/export/city",
      "dest": "/api/export/city"
    },
    {
      "src": "/api/export/heatmap",
      "dest": "/api/export/heatmap"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/dist/index.html"
    }
  ]
}